{
    "vtex.menu@2.x:submenu.accordion#descrition": {
        "children": [
            "vtex.menu@2.x:menu#descrition-accordion"
        ]
    },
    "vtex.menu@2.x:menu#descrition-accordion": {
        "children": [
            "flex-layout.col#especificaciones"
        ],
        "props": {
            "blockClass": [
                "contenedorDescripcionDos"
            ]
        }
    },
    "flex-layout.col#especificaciones": {
        "children": [
            "rich-text#tituloEspecificaciones",
            "product-brand#Ficha",
            "wine-specification-group"
        ]
    },
    "rich-text#tituloEspecificaciones": {
        "props": {
            "text": "Especificaciones",
            "blockClass": [
                "tituloEspe"
            ]
        }
    },
    "product-brand#Ficha": {
        "props": {
            "displayMode": "text",
            "blockClass": [
                "brandFicha"
            ]
        }
    },
    "product-specification-group": {
        "children": [
            "product-specification-text#group",
            "product-specification"
        ],
        "props": {
            "blockClass": [
                "contenidoGrupoTecnica"
            ]
        }
    },
    "product-specification": {
        "children": [
            "product-specification-text#specification",
            "product-specification-values"
        ]
    },
    "product-specification-values": {
        "children": [
            "product-specification-text#value"
        ]
    },
    "product-specification-text#group": {
        "props": {}
    },
    "product-specification-text#specification": {
        "props": {
            "isFirstSpecificationValue": false
        }
    },
    "product-specification-text#value": {
        "props": {
            "message": "{specificationValue}"
        }
    }
}