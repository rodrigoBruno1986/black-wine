            "fullWidth":true,
            "preventVerticalStretch":true
          }
      },
      "image#uno": {
        "props": {
          "blockClass": ["imgUno"],
          "src": "https://winesupply.vteximg.com.br/arquivos/imgtienda.png",
          "link": "#"
        }
      },
      "rich-text#uno": {
        "props": {
          "text": "Tienda Exclusiva",
          "blockClass": "fuenteUno"
        }
      },
      "rich-text#dos": {
        "props": {
          "text": "Johnnie Walker",
