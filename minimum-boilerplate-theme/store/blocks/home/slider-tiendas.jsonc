            "flex-layout.row#post13"
        ]
    },

    "flex-layout.row#post1": {
        "children": ["image#post1", "flex-layout.col#post1"],
        "props": {
            "blockClass": ["contenidoTienda"]
        }
    },
    "flex-layout.col#post1": {
        "children": ["rich-text#post1", "rich-text#post11"],
        "props": {
            "fullWidth": true,
            "preventVerticalStretch": true
        }
    },
    "image#post1": {
        "props": {
            "blockClass": ["imgUno"],
            "src": "https://winesupply.vteximg.com.br/arquivos/imgtienda.png",
            "link": "#"
        }
    },
    "rich-text#post1": {
        "props": {
            "text": "Tienda Exclusiva",
            "blockClass": "fuenteUno"
        }
    },
    "rich-text#post11": {
        "props": {
            "text": "Johnnie Walker",
            "blockClass": "fuenteSecundariaUno"
        }
    },

