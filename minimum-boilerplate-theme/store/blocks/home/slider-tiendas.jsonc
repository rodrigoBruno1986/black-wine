{
    "slider-layout#tienda": {
        "props": {
            "itemsPerPage": {
                "desktop": 6,
                "tablet": 1,
                "phone": 1
            },
            "autoplay": {
                "timeout": 5000,
                "stopOnHover": true
            },
            "infinite": true,
            "showNavigationArrows": "desktopOnly",
            "blockClass": "carousel",
            "navigationStep": 1
        },
        "children": [
            "flex-layout.row#post1",
            "flex-layout.row#post2",
            "flex-layout.row#post3",
            "flex-layout.row#post4",
            "flex-layout.row#post5",
            "flex-layout.row#post6",
