    "list-context.image-list#oficial-bemberg": {
        "children": ["slider-layout#oficial-bemberg-images"],
        "props": {
            "height": 650,
            "images": [
                {
                    "image": "https://winesupply.vteximg.com.br/arquivos/1.jpg",
