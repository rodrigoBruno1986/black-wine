{

  
  "flex-layout.row#barra-bodegas":{
    "children": [
      "flex-layout.row#col1",
      "flex-layout.row#col2",
      "flex-layout.row#col3"
    ],
    "props": {
      "preventHorizontalStretch": false,
      "preventVerticalStretch": false,
      "fullWidth":true,
      "blockClass": ["contenidoMediosdepago", "contenidoBarraBodegas"]
    }
  },

"flex-layout.row#col1": {
    "children": [
      "flex-layout.col#info"
    ],
    "props": {
      "blockClass": "mediosDePagos",
      "preventHorizontalStretch": true
  
    }
  },

  "flex-layout.col#info":{
    "children":[
      "rich-text#col2"],
      "props": {
        "blockClass": "contenidoTexto",
        "fullWidth":true
      }
  },
 
  "rich-text#col2": {
    "props": {
      "text": "DESCUBRÍ NUESTROS BENEFICIOS",
      "blockClass": ["textoAlternativoInfopagos","textoBarraBodega"]
    }
  },
  //BLOQUE CUOTAS
  "flex-layout.row#col2": {
    "children": [
      "flex-layout.col#columnatextodos"
    ],
    "props": {
      "blockClass": "mediosDePagos",
      "preventHorizontalStretch": true
  
    }
  },
  "flex-layout.col#columnatextodos":{
    "children":[
      "rich-text#dos"],
      "props": {
        "blockClass": "contenidoTexto",
        "fullWidth":true
      }
  },
 
  "rich-text#dos": {
    "props": {
      "text": "DESCUBRÍ UNA EXPERIENCIA UNICA",
      "blockClass": ["textoAlternativoInfopagos","textoBarraBodega"]
    }
  },


  //FIN BLOQUE CUOTAS
  
  //BLOQUE ENVIOS

  "flex-layout.row#col3": {
    "children": [
      "flex-layout.col#infoenvios"
    ],
    "props": {
      "blockClass": "mediosDePagos",
      "preventHorizontalStretch": true
  
    }
  },

  "flex-layout.col#infoenvios":{
    "children":["rich-text#card2envios"],
      "props": {
        "blockClass": ["contenidoTexto","textoBarraBodega"],
        "fullWidth":true
      }
  },
  "rich-text#card2envios": {
    "props": {
      "text": "SET DE PRODUCTOS ESPECIALES",
      "blockClass": ["textoAlternativoInfopagos","textoBarraBodega"]
    }
  }

}