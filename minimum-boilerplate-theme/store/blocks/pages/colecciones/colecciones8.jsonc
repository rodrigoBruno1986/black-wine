      "blocks": [
          "list-context.image-list#colecciones",
          "flex-layout.row#colecciones--container"

      ]
  },
  "list-context.image-list#colecciones": {
    "children": [
      "slider-layout#colecciones-images"
    ],
    "props": {
      "height": 650,
      "images": [
        {
          "image": "https://limpioonline.vteximg.com.br/arquivos/1.jpg",
          "mobileImage": "https://limpioonline.vteximg.com.br/arquivos/1mobile.jpg",
          "description": "descuentos"
        },
        {
          "image": "https://limpioonline.vteximg.com.br/arquivos/2.jpg",
          "mobileImage": "https://limpioonline.vteximg.com.br/arquivos/1mobile.jpg",
          "description": "descuentos"
        }
      ]
    }
  },
  "slider-layout#colecciones-images": {
    "props": {
      "autoplay": {
        "timeout": 7000,
        "stopOnHover": true
      },
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "showPaginationDots": "never",
      "showNavigationArrows": "always",
      "fullWidth": true
    }
  },
  "flex-layout.row#colecciones--container": {
      "children": [
          "flex-layout.col#colecciones"
      ],
      "props": {
          "blockClass": ["contenedorColecciones"]
