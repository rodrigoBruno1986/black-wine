        "children": ["info-card#colecciones", "info-card#coleccionesDos"],
        "props": {
            "blockClass": ["contenedorInfoCartColecciones"]
        }
    },
    "image#colecciones": {
        "props": {
            "src": "https://storecomponents.vteximg.com.br/arquivos/banner-infocard2.png",
            "blockClass": ["imagenRodri"],
            "maxHeight": 250
        }
    },
    "info-card#colecciones": {
        "props": {
            "id": "info-card-test",
            "textPosition": "center",
            "imageUrl": "https://storecomponents.vteximg.com.br/arquivos/banner-infocard2.png",
            "headline": "¿NO SAVES QUÉ VINO ELEGIR?",
            "subhead": "Descubrí los vinos que conbinan con tus gustos con nuestro Test de Paladar",
            "callToActionText": "Empezar Test",
